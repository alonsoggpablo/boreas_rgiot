      "type": "table",
      "title": "Families With Timeout",
      "datasource": "Prometheus",
      "targets": [{
        "expr": "family_timeout == 1",
        "legendFormat": "{{family}}"
      }],
      "columns": [
        {"text": "Family", "value": "family"}
      ],
      "gridPos": {"x": 0, "y": 6, "w": 24, "h": 6}
