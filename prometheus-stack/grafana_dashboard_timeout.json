{
  "id": null,
  "title": "Families Timeout (No Messages in Last Hour)",
  "tags": ["django", "iot", "prometheus"],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "type": "stat",
      "title": "Families No Message Count",
      "datasource": "Prometheus",
      "targets": [{
        "expr": "families_no_message_count",
        "legendFormat": "No Msg Count"
      }],
      "gridPos": {"x": 0, "y": 0, "w": 8, "h": 4}
    },
    {
      "type": "table",
      "title": "Timeout Families",
      "datasource": "Prometheus",
      "targets": [{
        "expr": "family_timeout == 1",
        "legendFormat": "{{family}}"
      }],
      "gridPos": {"x": 8, "y": 0, "w": 16, "h": 8}
    }
  ]
}
