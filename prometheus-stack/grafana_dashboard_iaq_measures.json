                        "rawQuery": true,
                        "rawSql": "SELECT\r\n  rm.report_time AS time,\r\n  (m->>'n') AS metric,\r\n  (m->>'v')::double precision AS value\r\nFROM boreas_mediacion_reported_measure rm\r\nCROSS JOIN LATERAL jsonb_array_elements(rm.measures->'measures') AS m\r\nWHERE rm.feed = 'IAQ_Measures'\r\n  AND rm.nanoenvi_name = '${device_name}'\r\n  AND (m->>'n') ~ '^(${IAQ_Measures:regex})$'\r\n  AND $__timeFilter(rm.report_time)\r\nORDER BY rm.report_time, (m->>'n');",
