{
  "id": null,
  "title": "Django IoT Metrics",
  "tags": ["django", "iot", "prometheus"],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "type": "stat",
      "title": "Families No Message Count",
      "datasource": "Prometheus",
      "targets": [{
        "expr": "families_no_message_count",
        "legendFormat": "No Msg Count"
      }],
      "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4}
    },
    {
      "type": "table",
      "title": "Family Timeout Status",
      "datasource": "Prometheus",
      "targets": [{
        "expr": "family_timeout",
        "legendFormat": "{{family}}"
      }],
      "gridPos": {"x": 6, "y": 0, "w": 12, "h": 8}
    },
    {
      "type": "table",
      "title": "Datadis Supply Timeout",
      "datasource": "Prometheus",
      "targets": [{
        "expr": "datadis_supply_timeout",
        "legendFormat": "{{cups}}"
      }],
      "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8}
    },
    {
      "type": "table",
      "title": "WirelessLogic SIM Timeout",
      "datasource": "Prometheus",
      "targets": [{
        "expr": "wirelesslogic_sim_timeout",
        "legendFormat": "{{iccid}}"
      }],
      "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8}
    },
    {
      "type": "table",
      "title": "Sigfox Device Timeout",
      "datasource": "Prometheus",
      "targets": [{
        "expr": "sigfox_device_timeout",
        "legendFormat": "{{device_id}}"
      }],
      "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8}
    }
  ]
}
